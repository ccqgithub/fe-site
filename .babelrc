{
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", {
      "modules": "commonjs",
      "targets": {
        "browsers": ["> 1%", "ie > 9"]
      },
      "useBuiltIns": "usage",
      "corejs": { "version": 3, "proposals": true }
    }]
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ],
  "env": {
    "development": {
      "plugins": []
    }
  }
}