{
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx",
          ".vue"
        ]
      }
    }
  },
  "plugins": [
    "prettier"
  ],
  "extends": [
    // "airbnb",
    "prettier",
    "prettier/react",
    "prettier/standard"
  ],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error"
  }
}